{
  "name": "aq-native-miniapp",
  "version": "1.0.0",
  "description": "AQSoft MiniApp Core React Native Library",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:aqsoftware/aq-miniapp-sdk.git"
  },
  "main": "build/index.js",
  "scripts": {
    "build": "npm run babel && npm run copyassets && npm run doc",
    "babel": "rm -rf ../../aq-native-miniapp/build && babel ./src --out-dir=../../aq-native-miniapp/build",
    "copyassets": "rm -rf ../../aq-native-miniapp/build/assets && cp -R ./src/assets ../../aq-native-miniapp/build/assets/",
    "watch": "babel --watch ./src --out-dir=../../aq-native-miniapp/build",
    "doc": "./node_modules/.bin/esdoc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "nodeify": "rn-nodeify --install crypto,url,stream,vm,process --hack"
  },
  "author": "Ryan C. Brozo",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.22.0",
    "flow-bin": "^0.41.0",
    "jsdoc": "^3.4.3",
    "rn-nodeify": "mvayngrib/rn-nodeify"
  },
  "peerDependencies": {
    "react": "16.0.0-alpha.6",
    "react-native": "^0.46.0"
  },
  "keywords": [
    "mini-app",
    "react-native"
  ],
  "dependencies": {
    "base64-js": "^1.2.1",
    "prop-types": "^15.5.10",
    "react-native-crypto": "^2.0.2",
    "react-native-randombytes": "^2.0.0"
  }
}
